#include <iostream>
#include <string>

using namespace std;

void nameOfProgramm() {
    /*setlocale(0, "ru");*/

    string programmName = "           Translate value ASCII To CHar";
    
    cout << programmName;
    cout << endl;
    
}

void fromASCIIValToChar() {
    

    int c;
    cout << "ASCII Value\tCharacter\n";
    for (c = 0; c <= 255; c++)
    {
        cout << c << "\t\t\t" << (char)c << endl;
    }
}




int main()
{
    nameOfProgramm();
    fromASCIIValToChar();


    system("pause");
    return 0;

}


